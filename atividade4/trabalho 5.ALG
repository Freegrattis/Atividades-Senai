Algoritmo "semnome"

Var
   ladoQuadrado: real
   areaQuadrado: real


Inicio
      escreval("Para calcular a area do quadrado, informe o comprimento de um dos lados")
      leia(ladoQuadrado)
      areaQuadrado <-  ladoQuadrado ^ 2
      escreval("A area do quadrado é: ", areaQuadrado)
fimAlgoritmo