Algoritmo "semnome"

Var
   alturaLata: real
   raioLata: real
   volume: real


Inicio
      escreval("Informe a altura da lata ")
      leia(alturaLata)
      escreval("Informe o raio da lata ")
      leia(raioLata)
      volume <- PI x (raioLata ^2) x alturaLata
      escreval("o volume da lata é ", volume)

Fimalgoritmo